# Use PHP7 as default
 AddHandler application/x-httpd-php70 .php
 
RewriteEngine on

RewriteCond %{SERVER_PORT} 80 
RewriteRule ^(.*)$ https://www.autofactorng.com/$1 [R=301,L]

#RewriteCond %{HTTP_HOST} ^autofactorng\.com$ [OR]
#RewriteCond %{HTTP_HOST} ^www\.autofactorng\.com$
#RewriteRule ^blog$ "http\:\/\/autofactorng\.blogspot\.com\.ng\/" [R=301,L]

RewriteCond %{HTTP_HOST} ^autofactorng\.com$ [OR]
RewriteCond %{HTTP_HOST} ^www\.autofactorng\.com$
#RewriteRule ^cybersale$ "http\:\/\/autofactorng\.com\/products\.php\?cat_id\=10" [R=301,L]






RewriteRule ^deals/([-\w]+)$ products.php?cat_id=$1
RewriteRule ^all/([-\w]+)$ products.php?cat_id=$1

RewriteRule ^([-\w]+)/([-\w]+)$ products.php?cat_id=$1&sub_cat_id=$2

RewriteRule ^([-\w]+)/([-\w]+)/(\d+)$ products.php?cat_id=$1&sub_cat_id=$2&page=$3 

RewriteRule ^([-\w]+)/([-\w]+).html$ product_desc.php?table=$1&slug=$2




RewriteRule ^([-\w]+).html$ product_desc.php?pn=$1


RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule (.+) $1.php [L,QSA]




#RewriteRule ^cybersale$ "http\:\/\/autofactorng\.com\/products\.php\?cat_id\=10" [R=301,L]

